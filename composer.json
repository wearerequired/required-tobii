{
  "name": "wearerequired/required-tobii",
  "type": "wordpress-plugin",
  "description": "An accessible, open-source lightbox for WordPress.",
  "license": "GPL-2.0-or-later",
  "authors": [
    {
      "name": "required",
      "email": "info@required.ch",
      "homepage": "https://required.com",
      "role": "Company"
    }
  ],
  "require": {
    "php": ">=7.4",
    "wearerequired/traduttore-registry": "^2.0"
  },
  "require-dev": {
    "dealerdirect/phpcodesniffer-composer-installer": "^0.7",
    "wearerequired/coding-standards": "^5.0"
  },
  "extra": {
    "branch-alias": {
      "dev-master": "1.x-dev"
    }
  },
  "autoload": {
    "psr-4": {
      "Required\\Tobii\\": "inc"
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "scripts": {
    "format": "vendor/bin/phpcbf --report-summary --report-source .",
    "lint": "vendor/bin/phpcs --report-summary --report-source ."
  }
}
